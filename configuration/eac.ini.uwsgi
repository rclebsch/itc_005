[uwsgi]
project = eac
base = /path/to/root/

chdir = %(base)/webroot
wsgi-file = %(base)/webroot/itc_005/wsgi.py

master = true
processes = 5

socket = /var/run/%(project).sock
chmod-socket = 664
vacuum = true

env LANG="en_US.utf8"
env LC_ALL="en_US.UTF-8"
env LC_LANG="en_US.UTF-8"

logto = /var/log/uwsgi/%n.log
